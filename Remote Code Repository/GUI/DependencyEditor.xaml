<Window x:Class="GUI.DependencyEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="DependencyEditor" Height="350" Width="350"  Padding="20" FontFamily="tahoma" Background="#ccc" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"></RowDefinition>

            <RowDefinition Height="200"></RowDefinition>
            <!-- label -->
            <RowDefinition Height="20"></RowDefinition>
            <!-- file list -->
        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0"  Padding="10 5" FontWeight="Bold" FontSize="14">Add Dependency</TextBlock>
        <ListBox Grid.Row="1"  x:Name="KeyList" SelectionMode="Multiple"  Margin="10">
            
        </ListBox>
        <Button Grid.Row="2"  Width="60" Click="Button_Click" Margin="263,46,19,-46">OK</Button>
    </Grid>
</Window>
